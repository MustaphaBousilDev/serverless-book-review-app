  "dependencies": {
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1692.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.156",
    "@types/node": "^24.8.1",
    "formidable": "^2.1.1",
    "path-loader": "^1.0.7",
